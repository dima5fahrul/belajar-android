<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".LatihanViewsAndViewGroup">

    <Button
        android:id="@+id/btn_move_to_linear_relative_frame_table_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/linear_realtive_frame_table_layout" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_to_constraint_layout"
        android:text="@string/constraint_layout" />
    
</LinearLayout>
